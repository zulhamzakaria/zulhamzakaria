@model List<SearchViewModel>

<div class="row">
    <div class="col-md-10">
        @foreach(var item in Model)
            {
                <div class="border rounded border-info p-1 w-100">
                    <img src="https://cdn.onlinewebfonts.com/svg/img_194915.png"
                         style="width:24px;height:24px;" />
                    <a class="btn-link" href="@item.Id">@item.Title</a>
                </div>
            }
    </div>
</div>