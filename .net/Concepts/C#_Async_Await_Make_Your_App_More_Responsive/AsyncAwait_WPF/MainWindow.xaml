<Window x:Class="AsyncAwait_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AsyncAwait_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="20">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="44*"></RowDefinition>
            <RowDefinition Height="26*"/>
            <RowDefinition Height="227*"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="1" Margin="10,0,10,0" FontSize="28">
            Simple Async App
        </TextBlock>
        <Button x:Name="executeSync" Grid.Row="2" Height="40" Margin="10,10,10,10" Click="executeSync_Click">Normal Execute</Button>
        <Button x:Name="executeAsync" Grid.Row="4" Height="40" Margin="10,0,11,222" Grid.RowSpan="3" Click="executeAsync_Click">Async Execute</Button>
        <TextBlock x:Name="resultsWindow" Grid.Row="10" Margin="10,30,10,10"/>
    </Grid>
</Window>
