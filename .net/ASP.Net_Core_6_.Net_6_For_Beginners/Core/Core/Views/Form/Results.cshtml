@{
    ViewBag.Title = "Form Results";
}

<h2 class="bg-primary text-white text-center p-2">
    Results
</h2>

<table class="table table-striped table-bordered">
    <tr>
        <th colspan="2">Form Data</th>
    </tr>
    @foreach (var key in TempData.Keys)
    {
        <tr>
            <td>@key</td>
            <td>@TempData[key]</td>
        </tr>
    }

</table>

<a asp-action="index" >Back To Form</a>